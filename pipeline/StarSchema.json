{
	"name": "StarSchema",
	"properties": {
		"activities": [
			{
				"name": "Copy_vlh",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "."
					},
					{
						"name": "Destination",
						"value": "dbo.DimActors"
					}
				],
				"typeProperties": {
					"source": {
						"type": "SqlDWSource",
						"sqlReaderQuery": "INSERT INTO [dbo].[DimActors]\r\nselect * from [external].[Actors]"
					},
					"sink": {
						"type": "SqlDWSink",
						"allowPolyBase": true,
						"polyBaseSettings": {
							"rejectValue": 100,
							"rejectType": "percentage",
							"rejectSampleValue": 1000,
							"useTypeDefault": true
						}
					},
					"enableStaging": true,
					"stagingSettings": {
						"linkedServiceName": {
							"referenceName": "linkedService1",
							"type": "LinkedServiceReference"
						},
						"path": "staging"
					},
					"enableSkipIncompatibleRow": true,
					"redirectIncompatibleRowSettings": {
						"linkedServiceName": {
							"referenceName": "linkedService1",
							"type": "LinkedServiceReference"
						},
						"path": "logging"
					}
				},
				"inputs": [
					{
						"referenceName": "SourceDataset_vlh",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DestinationDataset_vlh",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}